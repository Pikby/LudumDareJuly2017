if(dead = true) exit
if(max(keyboard_check_pressed(vk_escape),keyboard_check_pressed(ord('P'))))
{
    if(global.pause = true)
    {
        global.pause = false
    }
    else global.pause = true
}

if(global.pause = true) exit;
if(x&gt;room_width) room_goto_next()

moving = 0
if(keyboard_check(ord('S')))
{
        facing = curFace.DOWN;
    moving = 1
    if(!place_meeting(x,y+moveSpeed,objWall)) y+=moveSpeed;
}
if(keyboard_check(ord('W')))
{
    moving = 1
    facing = curFace.UP;
    if(!place_meeting(x,y-moveSpeed,objWall)) y-=moveSpeed;
}
if(keyboard_check(ord('A')))
{
    moving = 1
    facing = curFace.LEFT;
    if(!place_meeting(x-moveSpeed,y,objWall)) x-=moveSpeed;
}
if(keyboard_check(ord('D')))
{   
    moving = 1
    facing = curFace.RIGHT;
    if(!place_meeting(x+moveSpeed,y,objWall)) x+=moveSpeed;
}

if(keyboard_check(vk_space))
{

    
    if(energy &gt; 0)
    {
    damage = energy/maxEnergy
    beamSize = energy/maxEnergy * 16
    if(!audio_is_playing(lazerHold)) audio_play_sound(lazerHold,10,false)
    beamOn = true;
    energy -= 1;

    beamLength += 5;
    beamLength *= 2;
           
    if(beamLength &gt; 2000) beamLength = 2000;
    angle = degtorad(point_direction(x,y,mouse_x,mouse_y))
    x1 = x+cos(angle+pi/2)*beamSize;
    y1 = y-sin(angle+pi/2)*beamSize;
    x2 = x-cos(angle+pi/2)*beamSize;
    y2 = y+sin(angle+pi/2)*beamSize;
    
    for(i=1;i&lt;beamLength/16;i++)
    {
        tempx=x+cos(angle)*(i*16);
        tempy=y-sin(angle)*(i*16);
        
        if(place_meeting(tempx,tempy,objWall))
        {
            beamLength = (i)*16;   
        }
        angle = degtorad(point_direction(x,y,mouse_x,mouse_y)) 
        light = instance_create(x+cos(angle)*(i*16),y-sin(angle)*(16*i),objLightBeam)
        with(light) lightSize = other.energy/other.maxEnergy * 5
    }
    
    
    
    x3 = x1+cos(angle)*(beamLength+64);
    y3 = y1-sin(angle)*(beamLength+64);   
    x4 = x2+cos(angle)*(beamLength+64);
    y4 = y2-sin(angle)*(beamLength+64);

    if(x1&lt;x3) facing = curFace.RIGHT
    else facing = curFace.LEFT
    with(objZombie)
    {
        if(point_in_triangle(x,y,other.x1,other.y1,other.x2,other.y2,other.x3,other.y3) || point_in_triangle(x,y,other.x2,other.y2,other.x3,other.y3,other.x4,other.y4) )
        {
            hp -= other.damage;
            if(!audio_is_playing(sndZombieHit)) audio_play_sound(sndZombieHit,0,false)
        }
    }
    with(objCrate)
    {
        if(point_in_triangle(x,y,other.x1,other.y1,other.x2,other.y2,other.x3,other.y3) || point_in_triangle(x,y,other.x2,other.y2,other.x3,other.y3,other.x4,other.y4) )
        hp -= other.damage;
    }
    
    }
    else 
    {
    beamOn = false;      
    if(audio_is_playing(lazerHold)) audio_stop_sound(lazerHold)
    }
    x3 = x1+cos(angle)*(beamLength);
    y3 = y1-sin(angle)*(beamLength);   
    x4 = x2+cos(angle)*(beamLength);
    y4 = y2-sin(angle)*(beamLength);
}
else  if(audio_is_playing(lazerHold)) audio_stop_sound(lazerHold)
if(keyboard_check_released(vk_space))
{
    beamLength = 0;
    beamOn = false;
}

if(keyboard_check_pressed(ord('E')))
{
    switch(curGrenade)
    {
        case(grenade.FLARE):
        {
            flare = instance_create(x,y,objFlare)
            with(flare)
            {
              targetx = mouse_x;
              targety = mouse_y;
            }
            curGrenade = grenade.NONE;
        }
    }
}

if(keyboard_check_released(vk_delete))
{
    room_restart()
}
